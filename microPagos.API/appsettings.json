{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "conexion": "${DB_CONNECTION_STRING}",
    "Token": {
      "Llave": "${JWT_SECRET_KEY}"
    },
    "MercadoPago": {
      "ACCESS_TOKEN": "${MP_ACCESS_TOKEN}",
      "SuccessUrl": "${MP_SUCCESS_URL}",
      "FailureUrl": "${MP_FAILURE_URL}",
      "PendingUrl": "${MP_PENDING_URL}",
      "CallbackUrl": "${MP_CALLBACK_URL}"
    },
    "PEDIDOSAPI": {
      "url": "${PEDIDOS_API_URL}"
    }
  }
}
